<div class="responsive-list">
	<div class="row">
		{% set i=0 %}
		{% for col in range(1,3) %}
		<div class="col-sm-4">
			<ul>
				{% set curr=0 %}
				{% set total=(data|length/3)|round(0,'floor') %}
				{% if data|length%3>=col %}
				{% set total=total+1 %}
				{% endif %}
				{% if total!=0 %}
					{% for item in range(i,i+total-1) %}
						<li>{{data[item].content}}</li>
					{% endfor %}
				{% endif %}
				{% set i=i+total %}
			</ul>
		</div>
		{% endfor %}
	</div>
</div>
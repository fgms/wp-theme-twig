{# Style Settings #}
{% set body_background_color = '#f1f1f1' %}
{% set body_color = '#666666' %}
{% set body_font_family = 'Helvetica, Arial, sans-serif' %}
{% set heading_color = '#606060' %}
{% set heading_font_family = 'georgia,times,serif' %}
{% set container_background_color = '#ffffff' %}
{% set link_color = '#2ba6cb' %}
{% set link_color_hover = '#2795b6' %}
{% set title_styles = 'font-family: '~heading_font_family~'; font-size: 22px; line-height: 26px; color: '~heading_color~';' %}
{% set subtitle_styles = 'font-family: '~heading_font_family~'; font-style: italic; font-size: 18px; line-height: 24px; color: '~heading_color~';' %}
{% set body_text_styles = "font-family: 'Open Sans',arial,helvetica,sans-serif; font-size: 14px; line-height: 22px; color: "~body_color~';' %}
{% set panel_body_text_sytles = 'color: '~body_color~'; font-size: 13px; line-height: 16px;' %}
{% set footnote_styles = 'color: '~body-color~'; font-size: 12px; line-height: 16px;' %}
{% set footer_title_styles = 'font-size: 16px; line-height: 16px; color: '~heading_color~'; ' %}

{# image settings #}
{% set image = {
        header : {
            src: theme.link~'/wp-content/assets/images/email/email-header.jpg',
            alt: '[Image]'~site.description
        },
        footer : {
            src: theme.link~'/wp-content/assets/images/email/email-footer.jpg',
            alt: '[Image]'~site.description
        }
    }
%}

{# email content #}
{% set email = {
        company : config.company.name,
        title : 'Website Inquiry',
        message : 'The following message was submitted via the website '~site.url~':',
        phone : {
            office : config.phone.office,
            toll: config.phone.toll
        },
        email : config.email.info,
        address: {
            street : config.company.address.street,
            province : config.company.address.province,
            city : config.company.address.city,
            country : config.company.address.country
        },
        privacy : {
            title : 'Privacy Policy',
            url : function('get_permalink',1)
        },
        optoutaddress : 'webmaster@fifthgeardev.com'
    }
%}




